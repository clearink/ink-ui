{
  "name": "@ink-ui/core",
  "version": "0.0.8",
  "description": "ink-ui core components",
  "author": "clearink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clearink/ink-ui.git",
    "directory": "packages/components"
  },
  "keywords": [
    "ink-ui",
    "react",
    "components"
  ],
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "*.scss"
  ],
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.mjs",
      "require": "./lib/index.js"
    },
    "./esm/*.mjs": {
      "types": "./esm/*.d.ts",
      "import": "./esm/*.mjs"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./*": "./*"
  },
  "main": "./lib/index.js",
  "module": "./esm/index.mjs",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "types": "./esm/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "esm",
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "scripts": {
    "build": "ink-scripts build:core",
    "lint:ts": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "dependencies": {
    "@babel/runtime": "catalog:",
    "@ink-ui/icons": "workspace:^",
    "classnames": "^2.5.1",
    "react-fast-compare": "^3.2.0",
    "react-is": "^18.3.1"
  },
  "devDependencies": {
    "@internal/scripts": "workspace:^",
    "@types/react-is": "^18.3.0"
  }
}
